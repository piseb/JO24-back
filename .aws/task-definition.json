{
  "containerDefinitions": [
    {
      "name": "jo24-back-container",
      "image": "885470171601.dkr.ecr.eu-west-3.amazonaws.com/github-repo:jo24.back.479e4e33d7414b5a8a0e94aac268ebb82177135f",
      "cpu": 0,
      "portMappings": [
        {
          "name": "jo24-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "secrets": [
        {
          "name": "aws_settings",
          "valueFrom": "arn:aws:secretsmanager:eu-west-3:885470171601:secret:jo24-settings-secrets-nfLfgi"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/jo24-back-task",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "eu-west-3",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "family": "jo24-back-task",
  "executionRoleArn": "arn:aws:iam::885470171601:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}