{
  "containerDefinitions": [
    {
      "name": "jo24-back-dev-container",
      "image": "885470171601.dkr.ecr.eu-west-3.amazonaws.com/jo24-back-dev-repo:jo24.back.7c8d3e475b70e6d35bae7f9fc72d02c9004f3e9f",
      "cpu": 512,
      "memory": 1024,
      "essential": true,
      "secrets": [
        {
          "name": "aws_settings",
          "valueFrom": "arn:aws:secretsmanager:eu-west-3:885470171601:secret:jo24-settings-secrets-nfLfgi"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/jo24-back_caddy-task",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "eu-west-3",
          "awslogs-stream-prefix": "ecs"
        }
      }
    },
    {
      "name": "caddy",
      "image": "885470171601.dkr.ecr.eu-west-3.amazonaws.com/jo24-back-caddy-repo:jo24.caddy.f6412ccd9dbfa57ca713e216d3a9f1fb0587b7f2",
      "cpu": 512,
      "memory": 1024,
      "essential": true,
      "portMappings": [
        {
          "name": "caddy-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/jo24-back_caddy-task",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "eu-west-3",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "family": "jo24-back-dev-task",
  "executionRoleArn": "arn:aws:iam::885470171601:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "2048",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}